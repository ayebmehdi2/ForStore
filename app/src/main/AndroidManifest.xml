<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mehdi.boutique">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_shop"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_shop"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.mehdi.boutique.SplachScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ActivityAuth"/>
        <activity android:name=".CompleteCompte"
            android:theme="@style/AppThemeD"
            android:label="Complete profil" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".SplachScreen" />
        </activity>

        <activity android:name=".profile"
            android:theme="@style/AppThemeD"
            android:label="My profil" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".HomeActivity" />
        </activity>

        <activity android:name=".HomeActivity"/>


    </application>

</manifest>